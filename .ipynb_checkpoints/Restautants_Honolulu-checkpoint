      "if (allOptions) {\n",
      "ops = \"toolbar=1,resizable=1,location=1,status=1\";\n",
      "}\n",
      "else {\n",
      "menubar = 0;\n",
      "ops = \"toolbar=0,resizable=1,location=0,status=0\";\n",
      "}\n",
      "ops += \",menubar=\"+menubar;\n",
      "ops += \",scrollbars=\"+noScrollVal;\n",
      "ops += \",width=\"+width;\n",
      "ops += \",height=\"+height;\n",
      "ops += \",screenX=\"+x+\",left=\"+x;\n",
      "ops += \",screenY=\"+y+\",top=\"+y;\n",
      "var w = window.open(url, winName, ops);\n",
      "if (w != null) {\n",
      "try {\n",
      "w.focus();\n",
      "w.opener = self;\n",
      "} catch (exc) {}\n",
      "return w;\n",
      "}\n",
      "} catch(e) {\n",
      "taError.record(e, 'Popup.open', null, { name: name, width: width, height: height, x: x, y: y, noScroll: noScroll, allOptions: allOptions});\n",
      "}\n",
      "},\n",
      "openExternalWithDefaultOptions: function(url) {\n",
      "try {\n",
      "var width = 475;\n",
      "var height = 390;\n",
      "var x = 210;\n",
      "var y = 5;\n",
      "if ( screen.width >= 1024 && screen.height >= 768) {\n",
      "width = 900;\n",
      "height = 768;\n",
      "x = 240;\n",
      "}\n",
      "else if (screen.width > 1024) {\n",
      "width = 800;\n",
      "height = 600;\n",
      "x = 240;\n",
      "}\n",
      "else if (screen.width > 800) {\n",
      "width = 620;\n",
      "height = 500;\n",
      "x = 240;\n",
      "}\n",
      "if (url.indexOf(\"p=HotelsCom\") > -1) {\n",
      "width = Math.max(width, 960);\n",
      "height = Math.max(\n",
      "height,\n",
      "window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight,\n",
      "600);\n",
      "}\n",
      "var noScroll = false;\n",
      "if (taBrowser.isChrome()) {\n",
      "noScroll = true;\n",
      "}\n",
      "var noScrollVal = 1;\n",
      "if(noScroll) {\n",
      "noScrollVal = 0;\n",
      "}\n",
      "var menubar = 1;\n",
      "if ( taBrowser.isIE9orHigher() )\n",
      "{\n",
      "var menubar = 0;\n",
      "}\n",
      "var popNum = _index++;\n",
      "var winName = \"TripPop\" + popNum;\n",
      "var ops = \"toolbar=1,resizable=1,location=1,status=1\";\n",
      "ops += \",menubar=\"+menubar;\n",
      "ops += \",scrollbars=\"+noScrollVal;\n",
      "ops += \",width=\"+width;\n",
      "ops += \",height=\"+height;\n",
      "ops += \",screenX=\"+x+\",left=\"+x;\n",
      "ops += \",screenY=\"+y+\",top=\"+y;\n",
      "var w = window.open(url, winName, ops);\n",
      "if (w != null) {\n",
